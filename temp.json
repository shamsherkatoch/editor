
{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "routeTables_rt_name": {
            "defaultValue": "rt-auac-dev-name",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/routeTables",
            "apiVersion": "2020-08-01",
            "name": "[parameters('routeTables_rt_name')]",
            "location": "australiacentral",
            "properties": {
                "disableBgpRoutePropagation": true,
                "routes": [
                    {
                        "name": "AGDNET",
                        "properties": {
                            "addressPrefix": "10.5.0.0/16",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "AGDNETNet",
                        "properties": {
                            "addressPrefix": "10.55.0.0/16",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "AGDNETNet2",
                        "properties": {
                            "addressPrefix": "10.6.3.0/24",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "AGDNETNet3",
                        "properties": {
                            "addressPrefix": "10.6.2.0/24",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "MGT",
                        "properties": {
                            "addressPrefix": "10.80.1.0/24",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "AGSES",
                        "properties": {
                            "addressPrefix": "172.20.0.0/16",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "Azureauac",
                        "properties": {
                            "addressPrefix": "10.61.0.0/20",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "10.61.0.12",
                            "hasBgpOverride": false
                        }
                    },
                    {
                        "name": "Default",
                        "properties": {
                            "addressPrefix": "0.0.0.0/0",
                            "nextHopType": "Internet",
                            "hasBgpOverride": false
                        }
                    }
                ]
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/AGDNET')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.5.0.0/16",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/AGDNETNet')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.55.0.0/16",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/AGDNETNet2')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.6.3.0/24",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/AGDNETNet3')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.6.2.0/24",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/AGSES')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "172.20.0.0/16",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/Azureauac')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.61.0.0/20",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/MGT')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.80.1.0/24",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "10.61.0.12",
                "hasBgpOverride": false
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2020-08-01",
            "name": "[concat(parameters('routeTables_rt_name'), '/Default')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_rt_name'))]"
            ],
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "Internet",
                "hasBgpOverride": false
            }
        }
    ]
}
